# environment variables for the API
NEQUI_DATABASE_URL=sqlite:///./app.db
# Set a non-empty API key to enable API key auth (header X-API-Key)
NEQUI_API_KEY=
# Comma-separated list of banned words
NEQUI_BANNED_WORDS=foo,bar,baz
# Optional naive per-minute rate limit (0 disables)
NEQUI_RATE_LIMIT_PER_MIN=0
